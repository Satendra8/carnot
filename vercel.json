{
    "builds": [
        {
            "src": "carnot/wsgi.py",
            "use": "@vercel/python"
        },
        {
            "src": "build_files.sh",
            "use": "@vercel/static-build"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "carnot/wsgi.py"
        }
    ]
}